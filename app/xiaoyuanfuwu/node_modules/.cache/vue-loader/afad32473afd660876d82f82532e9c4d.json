      }\n    });\n  },\n  mounted: function() {},\n\n  activated: function() {\n    //            if (!!this.scrollOffset && this.scrollOffset > 0) {\n    //                this.scrollView.scrollTop = this.scrollOffset;\n    //            }\n  },\n  methods: {\n    baoming(token = (this.token = this.work_id)) {\n      let jsonData = {\n        work_id: this.message.work_id,\n        token: token\n      };\n      this.$post(baoming, jsonData).then(res => {\n        this.$showDialog(true);\n        if (res.errno == 0) {\n          this.$showDialog(false);\n          this.$dialog.toast({\n            mes: '报名成功',\n            timeout: 1500,\n            icon: 'success'\n          });\n          this.onPop();\n        } else {\n          this.$showError(res.errmsg);\n        }\n      });\n    },\n    onPop() {\n      this.$pop();\n    },\n    onPressPush(rou) {\n      this.$pushRoute(rou);\n    }\n  }\n};\n</script>\n\n<style scoped></style>\n"]}]}