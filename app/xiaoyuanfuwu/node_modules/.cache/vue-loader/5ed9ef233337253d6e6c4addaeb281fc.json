ice() {\n      this.price = 0;\n      for (var i = 0; i < this.checklist3.length; i++) {\n        this.price += Number(this.checklist3[i]);\n      }\n    },\n    /**\n     * 加入购物车成功\n     */\n    toastSuccess() {\n      this.$dialog.toast({\n        mes: '加入成功',\n        timeout: 1500,\n        icon: 'success'\n      });\n    }\n  }\n};\n</script>\n\n<style scoped>\n.footer {\n  height: 2rem;\n  padding-bottom: 2rem;\n  padding-top: 0.3rem;\n  text-align: center;\n}\n.demo-checklist-img {\n  width: 2rem;\n}\n</style>\n"]}]}