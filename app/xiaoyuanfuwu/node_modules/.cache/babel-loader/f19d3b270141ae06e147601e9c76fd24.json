  } else {\n        _this.$showError(res.errmsg);\n      }\n    });\n  },\n  mounted: function mounted() {},\n  activated: function activated() {},\n  updated: function updated() {},\n  methods: {\n    onPressPush: function onPressPush() {\n      this.$pushRoute('details');\n    },\n    onPop: function onPop() {\n      this.$pop();\n    },\n    //选择\n    change: function change(value, isCheckAll) {\n      this.isCheckAll = isCheckAll;\n      this.totalPrice();\n    },\n    checkAll: function checkAll() {\n      this.isCheckAll = !this.isCheckAll;\n      this.$refs.checklistDemo.$emit('ydui.checklist.checkall', this.isCheckAll);\n      this.totalPrice();\n    },\n    //计算总价\n    totalPrice: function totalPrice() {\n      this.price = 0;\n\n      for (var i = 0; i < this.checklist3.length; i++) {\n        console.log(this.checklist3[i]);\n        this.price += Number(this.checklist3[i]);\n      }\n    },\n\n    /**\n     * 加入购物车成功\n     */\n    toastSuccess: function toastSuccess() {\n      this.$dialog.toast({\n        mes: '加入成功',\n        timeout: 1500,\n        icon: 'success'\n      });\n    }\n  }\n};",{"version":3,"sources":["gouwuche.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyDA,SAAA,gBAAA,QAAA,mCAAA;AACA,OAAA,6BAAA;AACA,eAAA;AACA,EAAA,IAAA,EAAA,QADA;AAEA,EAAA,UAAA,EAAA,EAFA;AAGA,EAAA,KAAA,EAAA,EAHA;AAIA,EAAA,IAAA,EAAA,gBAAA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,MAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CADA;AAEA;AACA,MAAA,KAAA,EAAA,CAHA;AAIA;AACA,MAAA,UAAA,EAAA,EALA;AAMA,MAAA,UAAA,EAAA,KANA;AAOA,MAAA,YAAA,EAAA;AAPA,KAAA;AASA,GAdA;AAeA,EAAA,QAAA,EAAA,EAfA;AAgBA,EAAA,OAAA,EAAA,mBAAA;AAAA;;AACA,QAAA,GAAA,GAAA;AACA,MAAA,KAAA,EAAA,KAAA;AADA,KAAA,CADA,CAIA;;AACA,SAAA,KAAA,CAAA,gBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,KAAA,CAAA,WAAA,CAAA,IAAA;;AACA,UAAA,GAAA,CAAA,KAAA,IAAA,CAAA,EAAA;AACA,QAAA,KAAA,CAAA,WAAA,CAAA,KAAA;;AACA,QAAA,KAAA,CAAA,YAAA,GAAA,GAAA,CAAA,IAAA;AACA,OAHA,MAGA;AACA,QAAA,KAAA,CAAA,UAAA,CAAA,GAAA,CAAA,MAAA