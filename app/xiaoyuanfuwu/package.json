{
  "name": "frameworkd-3.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve --mode dev",
    "build": "vue-cli-service build --mode pro",
    "lint": "vue-cli-service lint",
    "format": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "js-md5": "^0.7.3",
    "vue": "^2.5.21",
    "vue-awesome-swiper": "^3.1.3",
    "vue-router": "^3.0.2",
    "vue-ydui": "git+https://github.com/wzf/vue-ydui.git",
    "vuex": "^3.1.0",
    "ydui-district": "^1.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.16.2",
    "prettier-eslint-cli": "^4.7.1",
    "qs": "^6.6.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 0,
      "prettier/prettier": [
        "error"
      ],
      "no-empty": [
        "error",
        {
          "allowEmptyCatch": true
        }
      ],
      "no-else-return": 2
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8",
    "iOS >= 10",
    "Android >=4.4"
  ]
}
